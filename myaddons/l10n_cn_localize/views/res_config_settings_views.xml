<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.l10n_cn.localize</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="0"/>
            <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <div id="unsplash" position='after'>
                    <div class="col-12 col-lg-6 o_setting_box" id="localize_amap">
                        <div class="o_setting_left_pane">
                            <field name="module_l10n_cn_localize_amap"/>
                        </div>
                        <div class="o_setting_right_pane" id="localize_amap_settings">
                            <label for="module_l10n_cn_localize_amap"/>
                            <div class="text-muted">
                                Sync Chinese cities with Amap Service
                            </div>
                        </div>
                    </div>
                </div>
            </field>
        </record>
    </data>
</odoo>